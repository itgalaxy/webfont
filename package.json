{
  "name": "webfont",
  "version": "8.2.0",
  "description": "Generator of fonts from svg icons, svg icons to svg font, svg font to ttf, ttf to eot, ttf to woff, ttf to woff2",
  "license": "MIT",
  "author": "itgalaxy <development@itgalaxy.company>",
  "contributors": [
    {
      "name": "Alexander Krasnoyarov",
      "email": "alexander.krasnoyarov@itgalaxy.company",
      "url": "https://vk.com/sterling_archer"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/itgalaxy/webfont.git"
  },
  "homepage": "https://github.com/itgalaxy/webfont",
  "bugs": "https://github.com/itgalaxy/webfonty/issues",
  "keywords": [
    "cli",
    "standalone",
    "font",
    "fonts",
    "webfont",
    "webfonts",
    "svg",
    "ttf",
    "woff",
    "woff2",
    "otf",
    "ttf",
    "woff",
    "eot",
    "svg",
    "ttf2eot",
    "ttf2woff",
    "ttf2svg",
    "svg2ttf",
    "css",
    "scss",
    "builder",
    "generator"
  ],
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "files": [
    "dist",
    "templates",
    "!**/__tests__"
  ],
  "dependencies": {
    "async-throttle": "^1.1.0",
    "cosmiconfig": "^5.0.3",
    "xml2js": "^0.4.17",
    "globby": "^8.0.1",
    "meow": "^5.0.0",
    "fs-extra": "^7.0.1",
    "lodash.merge": "^4.6.1",
    "nunjucks": "^3.0.0",
    "resolve-from": "^4.0.0",
    "svg2ttf": "^4.0.0",
    "svgicons2svgfont": "^9.0.3",
    "ttf2eot": "^2.0.0",
    "ttf2woff": "^2.0.0",
    "ttf2woff2": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.9.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.0",
    "execa": "^1.0.0",
    "eslint": "^5.9.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-itgalaxy": "^88.0.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-lodash": "^3.1.0",
    "eslint-plugin-markdown": "^1.0.0-rc.1",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-unicorn": "^6.0.1",
    "is-eot": "^1.0.0",
    "is-svg": "^3.0.0",
    "is-ttf": "^0.2.0",
    "is-woff": "^1.0.0",
    "is-woff2": "^1.0.0",
    "npmpub": "^4.1.0",
    "npm-run-all": "^4.0.0",
    "nyc": "^13.1.0",
    "prettier": "^1.7.4",
    "remark-cli": "^6.0.0",
    "remark-preset-lint-itgalaxy": "^13.0.0",
    "rimraf": "^2.5.2",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4"
  },
  "scripts": {
    "lint:prettier": "prettier --list-different '{**/*,*}.{js,mjs,jsx,json,md,yml,yaml}' --ignore-path .gitignore",
    "lint:js": "eslint . --report-unused-disable-directives --ignore-path .gitignore --ext 'js,.mjs,.jsx,.md'",
    "lint:md": "remark . -i .gitignore -f -q",
    "lint": "npm-run-all -l --parallel lint:*",
    "prettify": "npm run lint:prettier -- --write",
    "fix:js": "npm run lint:js -- --fix",
    "fix": "npm-run-all -l prettify -p 'fix:**'",
    "pretest": "npm run lint",
    "test-only": "nyc ava --verbose",
    "test": "npm run test-only",
    "prebuild": "rimraf dist",
    "demo": "node dist/cli.js './src/__tests__/fixtures/svg-icons/*.svg' -d demo -t html --normalize --center-horizontally",
    "build": "babel src --out-dir dist --ignore /__tests__/",
    "prepublish": "npm-run-all build",
    "release": "npmpub"
  },
  "ava": {
    "require": "babel-register",
    "concurrency": 5,
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6.9.5"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "script"
    },
    "extends": [
      "plugin:itgalaxy/ava",
      "plugin:itgalaxy/node",
      "plugin:itgalaxy/esnext",
      "plugin:itgalaxy/markdown"
    ],
    "overrides": [
      {
        "files": [
          "src/**",
          "**/__tests__/**/*.js",
          "**/*.md"
        ],
        "parserOptions": {
          "sourceType": "module"
        },
        "rules": {
          "import/no-unresolved": [
            "error",
            {
              "commonjs": true,
              "caseSensitive": true,
              "ignore": [
                "webfont"
              ]
            }
          ],
          "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "never",
              "jsx": "never"
            }
          ],
          "node/no-unsupported-features/es-syntax": "off"
        }
      }
    ],
    "root": true
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-itgalaxy"
    ]
  },
  "engines": {
    "node": ">= 6.9.5 || >= 8.9.0"
  }
}
